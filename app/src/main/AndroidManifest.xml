<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cxh.busdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".eventbus.BActivity"/>
        <activity android:name=".eventbus.CActivity"/>
        <activity android:name=".otto.BActivity"/>
        <activity android:name=".otto.CActivity"/>
        <activity android:name=".rxbusold.BActivity"/>
        <activity android:name=".rxbusold.CActivity"/>
        <activity android:name=".messenger.BActivity"/>
        <activity android:name=".messenger.CActivity"/>
        <activity android:name=".rxbus.BActivity"/>
        <activity android:name=".rxbus.CActivity"/>
    </application>

</manifest>